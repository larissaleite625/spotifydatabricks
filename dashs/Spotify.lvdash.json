{
  "datasets": [
    {
      "name": "7edf2d93",
      "displayName": "total_sum_tempo",
      "queryLines": [
        "SELECT\r\n",
        "  TO_DATE(CONCAT(year, '-', month), 'yyyy-M') AS mes_ano,\r\n",
        "  SUM(hours_played) AS total_horas_ouvidas \r\n",
        "FROM\r\n",
        "  gold.spotify.streaming_history_com_estilos\r\n",
        "GROUP BY\r\n",
        "  mes_ano\r\n",
        "ORDER BY\r\n",
        "  mes_ano;"
      ]
    },
    {
      "name": "f367d883",
      "displayName": "top20_count",
      "queryLines": [
        "SELECT\r\n",
        "  master_metadata_album_artist_name,\r\n",
        "  COUNT(*) AS plays\r\n",
        "FROM\r\n",
        "  gold.spotify.streaming_history_com_estilos\r\n",
        "WHERE\r\n",
        "  master_metadata_album_artist_name not in ('null', 'The Red Army Choir')\r\n",
        "GROUP BY\r\n",
        "  master_metadata_album_artist_name\r\n",
        "ORDER BY\r\n",
        "  plays DESC\r\n",
        "LIMIT 20;"
      ]
    },
    {
      "name": "d329f988",
      "displayName": "top20_sum_hr",
      "queryLines": [
        "SELECT\r\n",
        "  master_metadata_album_artist_name,\r\n",
        "  SUM(minutes_played) AS total_horas_ouvidas\r\n",
        "FROM gold.spotify.streaming_history_com_estilos\r\n",
        "WHERE master_metadata_album_artist_name not in ('null', 'The Red Army Choir')\r\n",
        "GROUP BY master_metadata_album_artist_name\r\n",
        "ORDER BY total_horas_ouvidas DESC\r\n",
        "LIMIT 20;\r\n"
      ]
    },
    {
      "name": "3cb01b6b",
      "displayName": "Generos_musicais",
      "queryLines": [
        "SELECT\r\n",
        "  style,\r\n",
        "  sum(hours_played) AS horas_escutadas\r\n",
        "FROM\r\n",
        "gold.spotify.streaming_history_com_estilos\r\n",
        "WHERE\r\n",
        "  style <> 'unknown' or style IS NOT NULL\r\n",
        "GROUP BY\r\n",
        "  style\r\n",
        "ORDER BY\r\n",
        "  horas_escutadas DESC\r\n",
        "  limit 15;"
      ]
    },
    {
      "name": "f659f038",
      "displayName": "Estilos ao longo do tempo",
      "queryLines": [
        "WITH top_styles AS (\r\n",
        "  SELECT style, SUM(hours_played) AS horas\r\n",
        "  FROM gold.spotify.streaming_history_com_estilos\r\n",
        "  WHERE style <> 'unknown'\r\n",
        "  GROUP BY style\r\n",
        "  ORDER BY horas DESC\r\n",
        "  LIMIT 5\r\n",
        "),\r\n",
        "base AS (\r\n",
        "  SELECT date_trunc('month', from_utc_timestamp(ts,'America/Sao_Paulo')) AS mes,\r\n",
        "         s.style,\r\n",
        "         hours_played\r\n",
        "  FROM gold.spotify.streaming_history_com_estilos s\r\n",
        "  JOIN top_styles t ON s.style = t.style\r\n",
        ")\r\n",
        "SELECT mes, style, SUM(hours_played) AS minutos\r\n",
        "FROM base\r\n",
        "GROUP BY 1,2\r\n",
        "ORDER BY 1,2;\r\n"
      ]
    },
    {
      "name": "a93ca6de",
      "displayName": "Top 10 músicas",
      "queryLines": [
        "SELECT \r\n",
        "  master_metadata_track_name        AS faixa,\r\n",
        "  master_metadata_album_artist_name AS artista,\r\n",
        "  ROUND(SUM(hours_played), 2)  AS horas_ouvidas\r\n",
        "FROM gold.spotify.streaming_history_com_estilos\r\n",
        "where master_metadata_track_name not in ('National Anthem of USSR', 'null', 'Jorginho Me Empresta a 12')\r\n",
        "GROUP BY 1,2\r\n",
        "ORDER BY horas_ouvidas DESC\r\n",
        "LIMIT 20;\r\n"
      ]
    },
    {
      "name": "cf8c271e",
      "displayName": "Top 1 artista mais escutado do ano",
      "queryLines": [
        "SELECT \r\n",
        "  year,\r\n",
        "  master_metadata_album_artist_name AS artista,\r\n",
        "  ROUND(SUM(minutes_played)/60, 2)  AS horas_ouvidas,\r\n",
        "  CONCAT(CAST(year AS STRING), ' — ', master_metadata_album_artist_name) AS ano_artista,\r\n",
        "  RANK() OVER (PARTITION BY year ORDER BY SUM(minutes_played) DESC) AS rk\r\n",
        "FROM gold.spotify.streaming_history_com_estilos\r\n",
        "WHERE master_metadata_album_artist_name <> 'null'\r\n",
        "GROUP BY year, artista\r\n",
        "QUALIFY rk = 1\r\n",
        "ORDER BY year DESC;\r\n"
      ]
    }
  ],
